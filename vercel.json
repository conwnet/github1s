{
  "routes": [{
      "src": "/static/(.*)",
      "dest": "/static/$1",
      "headers": { "Cache-Control": "max-age=604800" }
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": ".*",
      "dest": "/index.html"
    }
  ]
}
