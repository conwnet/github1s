{
  "routes": [
    {
      "src": "/api/sourcegraph",
      "dest": "https://sourcegraph.com/.api/graphql"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1",
      "headers": {
        "Cache-Control": "max-age=604800"
      }
    },
    {
      "src": "/favicon(-dark|-light)?.ico",
      "dest": "/favicon$1.ico"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": ".*",
      "dest": "/index.html"
    }
  ]
}